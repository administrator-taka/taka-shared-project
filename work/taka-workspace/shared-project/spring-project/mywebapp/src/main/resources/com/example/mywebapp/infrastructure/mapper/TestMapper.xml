<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.mywebapp.infrastructure.mapper.TestMapper">
  <select id="selectTest" resultType="com.example.mywebapp.infrastructure.entity.TestEntity">
    select
    *
    from test t
    where t.name=#{name}
  </select>
</mapper>
